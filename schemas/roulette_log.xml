<table name="roulette_log">
    <column name="log_srl" type="number" size="11" notnull="notnull" primary_key="primary_key" auto_increment="auto_increment" />
    <column name="module_srl" type="number" size="11" notnull="notnull" default="0" />
    <column name="member_srl" type="number" size="11" notnull="notnull" index="idx_member_srl" />
    <column name="point_spent" type="number" size="11" notnull="notnull" default="0" />
    <column name="reward_text" type="varchar" size="250" default="" />
    <column name="reward_point" type="number" size="11" default="0" />
    <column name="regdate" type="date" index="idx_regdate" />
    <column name="ipaddress" type="varchar" size="128" />
</table>
